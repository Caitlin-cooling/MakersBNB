<%= erb :'partials/head' %>
<%= erb :'partials/nav_bar' %>
<h1 class='listing_pages_title_text'>Postings</h1>
<ul>
  <% @postings.each do |post| %>
  <li>
    <%= post.title %>
    <form action="/postings/<%=post.id%>" method="get">
      <input type="submit" value="Book">
    </form>
  </li>
  <% end %>
</ul>
